{
  "name": "@tamtt-labs/api-gateway",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "module": "src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "test": "bun test",
    "build": "bun build --minify-whitespace --minify-syntax --outfile ./dist/index.js src/index.ts",
    "start": "NODE_ENV=production bun ./dist/index.js",
    "dev": "bun run --watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "dependencies": {
    "elysia": "^1.4.25"
  },
  "devDependencies": {
    "@elysiajs/eden": "^1.4.8",
    "@tamtt-labs/tsconfig": "workspace:*",
    "@types/bun": "^1.3.9"
  }
}
